    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>PAD Panel</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        
        <link rel="stylesheet" href="/vendor/css/bootstrap.min.css">
        <link rel="stylesheet" href="/vendor/css/font-awesome.min.css">
        <link rel="stylesheet" href="/vendor/css/bootstrap-theme.min.css">
        <link rel="stylesheet" href="/vendor/css/xeditable.css">
        <link rel="stylesheet" href="/panel/css/panel.css">

        <script src="/vendor/js/modernizr/modernizr-2.6.2-respond-1.1.0.min.js"></script>
        <script src="/vendor/js/jquery/jquery-1.11.0.min.js"></script>
        <script src="/vendor/js/jquery/jquery.qrcode-0.11.0.min.js"></script>
        <script src="/vendor/js/qr/jsqrcode-combined.min.js"></script>
        <script src="/vendor/js/qr/html5-qrcode.js"></script>
        <script src="/vendor/js/bootstrap/bootstrap.min.js"></script>
        <script src="/vendor/js/underscore/underscore-min.js"></script>
        <script src="/vendor/js/angular/angular.min.js"></script>
        <script src="/vendor/js/angular/angular-resource.min.js"></script>
        <script src="/vendor/js/angular/angular-route.min.js"></script>
        <script src="/vendor/js/angular/xeditable.min.js"></script>

        <script src="/panel/js/panel.js"></script>

    </head>
    <body>
      <!--[if lt IE 8]>
          <p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
      <![endif]-->

      <div class="navbar navbar-inverse navbar-fixed-top" role="navigation" ng-controller="RepoListCtrl">
        <div class="container-fluid">
          <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse">
              <span class="sr-only">Toggle navigation</span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#"><i class="fa fa-dashboard"></i> PAD Panel</a>
          </div>
          <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav navbar-right">
              <li class="dropdown">
                <a class="dropdown-toggle pointer" data-toggle="dropdown">Repositorio <span class="caret"></span></a>
                <ul class="dropdown-menu" role="menu">
                  <li ng-class="(selectedRepo == repo) ? 'active' : ''" ng-repeat="repo in repos">
                    <a class="pointer" ng-click="changeRepo(repo)">{{repo}}</a>
                  </li>
                </ul>
              </li>
              <li><a href="#/share?repo={{selectedRepo}}">Compartir</a></li>
              <li><a href="#/repository/add?repo={{selectedRepo}}">Agregar un Repositorio</a></li>
              <li><a href="#/package/add?repo={{selectedRepo}}">Nuevo Tangible</a></li>
            </ul>
          </div>
        </div>
      </div>
      
      <div class="container-fluid">
        <div ng-view class="view-frame"></div>
      </div>
    </body>
</html>
